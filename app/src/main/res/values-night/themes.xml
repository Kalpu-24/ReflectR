<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.ReflectR" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:navigationBarColor">
            @android:color/transparent
        </item>
        <item name="android:statusBarColor">
            @android:color/transparent
        </item>
    </style>
    <style name="ThemeOverlay.AppTheme.TextInputLayout.Outlined" parent="">
        <item name="colorControlActivated">@color/material_dynamic_primary60</item>
    </style>
    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">38%</item>
    </style>
    <style name="BottomNavigationView">
        <item name="colorSecondaryContainer">@color/material_dynamic_primary90</item>
    </style>
    <style name="MyFAB">
        <item name="colorPrimaryContainer">@color/material_dynamic_primary95</item>
        <item name="colorOnPrimaryContainer">@color/material_dynamic_primary50</item>
    </style>
    <style name="slightRoundedImage" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
    </style>
</resources>